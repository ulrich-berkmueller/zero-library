{
    "name"       : "gwa/zero-library",
    "type"       : "library",
    "description": "A WordPress development framework.",
    "keywords"   : ["roots", "wordpress", "composer", "wp", "twig", "zero", "library", "zero-library"],
    "license"    : "MIT",
    "homepage"   : "https://github.com/ulrich-berkmueller/zero-library",
    "authors"    : [
        {
          "name"    : "Great White Ark",
          "homepage": "http://www.greatwhiteark.com"
        },
        {
            "name" : "Ulrich Berkmueller, Ideal Bytes"
        }
    ],
    "require": {
        "php"                                  : ">=8.3",
        "composer/installers"                  : "^1.0 || ^2.3",
        "jjgrainger/posttypes"                 : "^2.0",
        "timber/timber"                        : "^2.3.3",
        "cmb2/cmb2"                            : "^2.11",
        "roots/soil"                           : "^4.1.1"
    },
    "require-dev": {
        "phpunit/phpunit"                      : "^11.5.46",
        "mockery/mockery"                      : "^1.6.12",
        "friendsofphp/php-cs-fixer"            : "^3.92"
    },
    "autoload": {
        "psr-4": {
            "Gwa\\Wordpress\\Zero\\"           : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gwa\\Wordpress\\Zero\\Test\\"     : "tests/"
        },
        "classmap": [
            "wp-content/plugins/soil"
        ]
    },
    "extra": {
        "branch-alias": {
        }
    },
    "minimum-stability"                        : "dev",
    "prefer-stable"                            : true,
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
